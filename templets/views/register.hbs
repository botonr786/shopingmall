{{>header}}
<div class="container">
    <div class="row">
        <div class="col-md-12">
            {{>navbar}}
        </div>
    </div>
</div>
<div class="container">
    <div class="row">
        <div class="col-md-6">
            <form action="/register" method="POST">
            <input type="text" placeholder="Enter The First Name" class="form-control" name="first"><br>
            <input type="text" placeholder="Enter The Last Name" class="form-control" name="last"><br>
            <input type="email" placeholder="Enter The Gmail Id" class="form-control" name="gmail"><br>
            <input type="text" placeholder="Enter The Phone Number" class="form-control" name="number"><br>
            <input type="password" placeholder="Enter The Password" class="form-control" name="password"><br>
            <input type="password" placeholder="Enter The Conform Password" class="form-control" name="conpass"><br>
            <input type="submit" class="btn btn-success" value="Register">
            </form>
        </div>
    </div>
</div>